{
  "manifest_version": 3,
  "name": "Enter押しても送信しない君",
  "description": "Chat GPTにて、Enterは改行、Control+Enterは送信となります。",
  "version": "1.0",
  "icons": {
    "16": "images/enter_return_16.png",
    "32": "images/enter_return_32.png",
    "48": "images/enter_return_48.png",
    "128": "images/enter_return_128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://chat.openai.com/*"]
    }
  ]
}
